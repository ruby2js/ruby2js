    "start:bun": "bun node_modules/ruby2js-rails/server.mjs",
    "start:deno": "deno run --allow-all node_modules/ruby2js-rails/server.mjs",
    "vite": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview"
  },
  "dependencies": {
    "ruby2js-rails": "file:../../../packages/ruby2js-rails",
    "ruby2js": "file:../../../demo/selfhost",
    "@hotwired/turbo": "^8.0.0",
    "@hotwired/stimulus": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.0",
    "reactflow": "^11.10.0"
  },
  "devDependencies": {
    "vite": "^6.0.0",
    "vite-plugin-ruby2js": "file:../../../packages/vite-plugin-ruby2js"
  }
