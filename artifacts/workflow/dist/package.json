{
  "name": "workflow",
  "version": "0.1.0",
  "type": "module",
  "description": "Rails-like app powered by Ruby2JS",
  "scripts": {
    "dev": "ruby2js-rails-dev",
    "dev:ruby": "ruby2js-rails-dev --ruby",
    "build": "ruby2js-rails-build",
    "migrate": "ruby2js-rails-migrate",
    "start": "npx serve -s -p 3000",
