{
  "name": "ruby2js-selfhost",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "ruby2js": "./ruby2js"
  },
  "scripts": {
    "test": "node transliteration_spec.mjs",
    "test:e2e": "node test_e2e.mjs",
    "test:pipeline": "node test_full_pipeline.mjs",
    "test:walker": "node test_transpiled_walker.mjs",
    "build": "npm run build:converter && npm run build:spec",
    "build:converter": "bundle exec ruby ../../lib/ruby2js/selfhost.rb > selfhost_converter.mjs",
    "build:spec": "bundle exec ruby ../../lib/ruby2js/selfhost.rb ../../spec/transliteration_spec.rb > transliteration_spec.mjs",
    "build:walker": "ruby transpile_walker.rb > transpiled_walker.mjs"
  },
  "keywords": ["ruby", "javascript", "transpiler"],
  "author": "Sam Ruby",
  "license": "MIT",
  "description": "Self-hosted Ruby2JS converter running in Node.js",
  "dependencies": {
    "@ruby/prism": "^1.6.0"
  }
}
