{
  "name": "ruby2js-rails",
  "version": "6.0.0-beta",
  "type": "module",
  "description": "Rails runtime adapters, targets, and build tools for Ruby2JS",
  "bin": {
    "ruby2js-rails-dev": "./dev-server.mjs",
    "ruby2js-rails-server": "./server.mjs",
    "ruby2js-rails-build": "./build.mjs"
  },
  "exports": {
    "./adapters/*": "./adapters/*",
    "./targets/*": "./targets/*",
    "./erb_runtime.mjs": "./erb_runtime.mjs",
    "./build.mjs": "./build.mjs",
    "./dev-server.mjs": "./dev-server.mjs",
    "./server.mjs": "./server.mjs"
  },
  "files": [
    "adapters/",
    "targets/",
    "erb_runtime.mjs",
    "build.mjs",
    "dev-server.mjs",
    "server.mjs"
  ],
  "keywords": [
    "ruby2js",
    "rails",
    "activerecord",
    "erb"
  ],
  "author": "Sam Ruby",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruby2js/ruby2js"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "js-yaml": "^4.1.0",
    "ws": "^8.14.2"
  }
}
