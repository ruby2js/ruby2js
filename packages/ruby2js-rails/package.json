{
  "name": "ruby2js-rails",
  "version": "6.0.0-beta",
  "type": "module",
  "scripts": {
    "test": "node --test 'test/**/*.mjs'"
  },
  "description": "Rails runtime adapters, targets, and build tools for Ruby2JS",
  "bin": {
    "ruby2js-rails-dev": "./dev-server.mjs",
    "ruby2js-rails-server": "./server.mjs",
    "ruby2js-rails-build": "./build.mjs",
    "ruby2js-rails-migrate": "./migrate.mjs"
  },
  "exports": {
    "./adapters/*": "./adapters/*",
    "./targets/*": "./targets/*",
    "./rails_base.js": "./rails_base.js",
    "./rails_server.js": "./rails_server.js",
    "./erb_runtime.mjs": "./erb_runtime.mjs",
    "./phlex_runtime.mjs": "./phlex_runtime.mjs",
    "./build.mjs": "./build.mjs",
    "./dev-server.mjs": "./dev-server.mjs",
    "./server.mjs": "./server.mjs",
    "./migrate.mjs": "./migrate.mjs",
    "./vite": "./vite.mjs",
    "./vite.mjs": "./vite.mjs"
  },
  "files": [
    "adapters/",
    "targets/",
    "rails_base.js",
    "rails_server.js",
    "erb_runtime.mjs",
    "phlex_runtime.mjs",
    "build.mjs",
    "dev-server.mjs",
    "server.mjs",
    "migrate.mjs",
    "vite.mjs"
  ],
  "keywords": [
    "ruby2js",
    "rails",
    "vite",
    "activerecord",
    "erb",
    "phlex",
    "stimulus"
  ],
  "author": "Sam Ruby",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruby2js/ruby2js"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "js-yaml": "^4.1.0",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0",
    "vite-plugin-ruby2js": "*",
    "ruby2js": "*"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "vite-plugin-ruby2js": {
      "optional": true
    },
    "ruby2js": {
      "optional": true
    }
  }
}
